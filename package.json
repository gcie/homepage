{
  "name": "korepetycje",
  "version": "1.2.2",
  "description": "Konsola podprzęsłowego korepetycji Mostowych",
  "scripts": {
    "start": "node korepetycje-server/dist/server.js",
    "build": "npm run setup-client && npm run setup-server",
    "build:prod": "npm run setup-client:prod && npm run setup-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-client": "cd korepetycje-client && ng serve --proxy-config proxy.conf.json",
    "start-server": "cd korepetycje-server && tsc && npm start",
    "serve-client": "cd korepetycje-client && ng build --watch",
    "setup-client": "cd korepetycje-client && ng build && cd ..",
    "setup-client:prod": "cd korepetycje-client && ng build --prod && cd ..",
    "setup-server": "cd korepetycje-server && tsc && cd ..",
    "heroku-postbuild": "npm run build:prod"
  },
  "author": "Grzegorz Ciesielski",
  "license": "MIT",
  "repository": "https://github.com/tao24/korepetycje.git",
