    "name": "homepage",
    "version": "1.2.4",
    "description": "Moja strona domowa",
    "scripts": {
        "start": "node server/dist/server.js",
        "build": "npm run setup-client && npm run setup-server",
        "build:prod": "npm run setup-client:prod && npm run setup-server",
        "test": "echo \"Error: no test specified\" && exit 1",
        "start-client": "cd client && ng serve --proxy-config proxy.conf.json --port 4250",
        "start-server": "cd server && tsc && npm start",
        "serve-client": "cd client && ng build --watch",
        "serve-server:build": "cd server && gulp",
        "serve-server": "nodemon",
        "setup-client": "cd client && ng build && cd ..",
        "setup-client:prod": "cd client && ng build --prod && cd ..",
        "setup-server": "cd server && tsc && cd ..",
        "heroku-postbuild": "npm run build:prod"
    },
