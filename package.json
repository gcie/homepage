{
  "name": "homepage",
  "version": "2.0.2",
  "description": "Moja strona domowa",
  "scripts": {
    "build": "yarn run build:frontend && yarn run build:backend",
    "build:backend": "yarn --cwd backend build",
    "build:frontend": "yarn --cwd frontend build",
    "format:backend": "cd backend && prettier --write \"src/**/*.ts\" \"test/**/*.ts\" && cd ..",
    "install:backend": "yarn --cwd backend install --production=false",
    "install:frontend": "yarn --cwd frontend install --production=false",
    "postinstall": "yarn run install:backend && yarn run install:frontend",
    "postversion": "git push --tags",
    "start": "yarn --cwd backend start",
    "start:backend": "cd backend && nest start --watch",
    "start:backend:debug": "cd backend && nest start --debug --watch",
    "serve:frontend": "yarn --cwd frontend serve",
    "start:frontend": "yarn --cwd frontend start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:backend": "jest",
    "test:backend:watch": "jest --watch",
    "test:backend:cov": "jest --coverage",
    "test:backend:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:backend:e2e": "jest --config ./test/jest-e2e.json"
  },
  "author": "Grzegorz Ciesielski",
  "license": "MIT",
  "repository": "https://github.com/tao24/homepage.git",
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  }
}
